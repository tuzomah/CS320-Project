<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>More+</title>
    <link rel="stylesheet" href="\stylesheets\homecss.css">
</head>
<body>
    <div class="header">
        <h1><a href="/home">Risky Battlesnakes</a></h1>
	<h2><%= username %> - <%= coins %>Â¢</h2>
        <div class="line"></div>
        <li class="divider2"></li>
        <div class="navigation">
            <ul>
                <li><a href="/snakes">Snakes and Ladders</a></li>
                <li class="divider"></li>
                <li><a href="/battleship">Battleship</a></li>
                <li class="divider"></li>
                <li><a href="/risk">Risk</a></li>
                <li class="divider"></li>
                <li><a href="/leaderboard"><z>Leaderboard</z></a></li>
                <li class="divider"></li>
                <li><a href="/credits">Credits</a></li
            </ul>
            <li class="divider2"></li>
            <img src="/images/Xmas.png" alt="Title" class="Xtree">
            <img src="/images/Duck.png" alt="Title" class="Duck">
            <a href="/login"><img src="/images/login.png" alt="Login" class="login"></a>
        </div>
    </div>
</body>
<div class="content">
  <div class="chart">
  <h1>Leaderboard</h1>
  <div class="table-data">
    <table border="1">
      <tr>
        <th>ID</th>
        <th>Username</th>
	<th>Coins</th>
        <th>Complete Games</th>
      </tr>
      <% if (userData.length !== 0) { %>
        <% userData.forEach(function(data, index) { %>
          <tr>
            <td><%= data.id %></td>
            <td><%= data.username %></td>
	    <td><%= data.coins %></td>
            <td><%= data.complete_games %></td>
          </tr>
        <% }); %>
      <% } else { %>
        <tr>
          <td colspan="7">No Data Found</td>
        </tr>
      <% } %>
    </table>
  </div>
  </div>
</div>
</html>